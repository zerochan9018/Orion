	No.Font = Enum.Font.Unknown
	No.Text = "No"
	No.TextColor3 = Color3.fromRGB(255, 255, 255)
	No.TextScaled = true
	No.TextSize = 14.000
	No.TextWrapped = true

	UICorner_7.CornerRadius = UDim.new(0, 5)
	UICorner_7.Parent = No

	Unload.Name = "Unload"
	Unload.Parent = CloseOrNo
	Unload.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Unload.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Unload.BorderSizePixel = 0
	Unload.Position = UDim2.new(0.523, 0,0.637, 0)
	Unload.Size = UDim2.new(0.462, 0,0.2, 0)
	Unload.Font = Enum.Font.Unknown
	Unload.Text = "Yes"
	Unload.TextColor3 = Color3.fromRGB(255, 255, 255)
	Unload.TextScaled = true
	Unload.TextSize = 14.000
	Unload.TextWrapped = true

	UICorner_8.CornerRadius = UDim.new(0, 5)
	UICorner_8.Parent = Unload
	--[[Local Script]]--
	MainBackground1.BackgroundTransparency = 0.15
	UnLoad.MouseButton1Click:Connect(function()
		Open.Visible = true
		MainBackground1.Visible = false
	end)
	Close.MouseButton1Click:Connect(function()
		CloseOrNo.Visible = true
	end)
	Open_2.MouseButton1Click:Connect(function()
		CloseOrNo.Visible = false
		Open.Visible = false
		MainBackground1.Visible = true
	end)
	No.MouseButton1Click:Connect(function()
		CloseOrNo.Visible = false
	end)
	Template.Visible = false
	TemplateTab.Visible = false
	Unload.MouseButton1Click:Connect(function()
		Orion:Destroy()
	end)	
	--[[New Ui]]
	local Section1 = {}
	function Section1.New()
		Section.Visible = true
		local Tab = {}
		
		local function ClearUi()
			local Ui = TemplateTab.Parent
			for _,v in Ui:GetChildren() do
				if v:IsA('Frame') then
					v.Visible = false
				end
			end
		end
		
		function Tab.NewTab(TabName)
			local NewButton = Template:Clone()
			local NewFrame = TemplateTab:Clone()
			NewButton.Parent= Section.Tab
			NewFrame.Parent = TabFrame
			NewButton.Text = TabName
			NewButton.Visible = true
			NewButton.Name = TabName
			NewFrame.Name = TabName
			NewButton.MouseButton1Click:Connect(function()
				ClearUi()
				wait(0.2)
				print('New Tab')

				NewFrame.Visible = true
			end)
			print('New Tab Created')
			
			
			local Tabbest = {}

			function Tabbest.SetScroll(Number)
				NewFrame.ScrollingFrame.CanvasSize = UDim2.new(0,Number)
			end
			function Tabbest.AddButton(ButtonName,Callback)
				local Tombol = ButtonTemplate:Clone()
				Tombol.Parent = NewFrame.ScrollingFrame.Scaler.Tablist
				Tombol.ButtonText.Text = ButtonName
				Tombol.Name = ButtonName
				Tombol.Visible = true
				Tombol.Size = UDim2.new(1.018, 0,0.014, 0)
				local hit:TextButton = Tombol.HitBox
				hit.MouseButton1Click:Connect(function()
					Callback()
				end)
			end
			function Tabbest.AddToggle(ToggleName,CostumeBool,Callback)
				local Tombol =ToggleTemplate:Clone()
				local Toggler = Instance.new('BoolValue',Tombol)
				Toggler.Name = "Bool"
				Tombol.Parent = NewFrame.ScrollingFrame.Scaler.Tablist
				Tombol.ToggleText.Text = ToggleName
				Tombol.Visible = true
				Tombol.Name = ToggleName
				Tombol.Size = UDim2.new(1.018, 0,0.014, 0)

				local hit:TextButton = Tombol.Logo
				hit.MouseButton1Click:Connect(function()
					if Toggler.Value == false then
						Toggler.Value = true
						Tombol.Logo.BackgroundColor3 = Color3.new(0.333333, 1, 0)
						Callback(Toggler.Value)
					else
						Tombol.Logo.BackgroundColor3 = Color3.new(1, 0, 0)
						Toggler.Value = false
						Callback(Toggler.Value)
					end
				end)
			end
			
			
			
			
			return Tabbest
			
		end
		return Tab
	end	
	
	return Section1
end
